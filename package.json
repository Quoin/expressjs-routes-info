{
  "name": "@quoin/expressjs-routes-info",
  "version": "0.2.1",
  "description": "ExpressJS named routes",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "prepare": "npm run -s lint",
    "prepack": "npm run -s coverage",
    "lint": "npm run -s lint:node && npm run -s lint:test",
    "lint:node": "eslint --fix --ignore-pattern '*.test.js' lib",
    "lint:test": "eslint --fix --config=.eslintrc.test.json 'lib/**/*.test.js'",
    "coverage": "npm run -s coverage:unit",
    "coverage:unit": "nyc --report-dir=reports/coverage-unit npm run -s test:unit",
    "test": "npm run -s test:unit",
    "test:unit": "mocha \"lib/**/*.unit.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/expressjs-routes-info.git"
  },
  "keywords": [
    "ExpressJS",
    "routes",
    "resolver"
  ],
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "HuuDa.Tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/expressjs-routes-info/issues"
  },
  "homepage": "https://github.com/Quoin/expressjs-routes-info#readme",
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "dependencies": {
    "lodash": "~4.17.11",
    "url-template": "2.0.8"
  },
  "devDependencies": {
    "@quoin/node-test-helpers": "latest",
    "eslint": "~5.10.0",
    "eslint-config-standard": "~12.0.0",
    "eslint-plugin-html": "~5.0.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-node": "~8.0.0",
    "eslint-plugin-promise": "~4.0.1",
    "eslint-plugin-standard": "~4.0.0",
    "express": "4.16.x",
    "mocha": "~5.2.0",
    "nyc": "~13.1.0"
  }
}
